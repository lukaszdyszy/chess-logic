{
	"name": "chess-logic",
	"version": "0.0.1",
	"description": "A typescript library for chess logic like move validation etc.",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"scripts": {
		"build": "rm -rf dist/ && npm run build:esm && npm run build:cjs",
		"build:esm": "tsc",
		"build:cjs": "tsc --module CommonJS --outDir dist/cjs",
		"dev": "tsc --watch",
		"test": "jest"
	},
	"keywords": [
		"chess",
		"game",
		"move-validation",
		"chessboard",
		"chess-game",
		"FEN",
		"PGN"
	],
	"author": "lukaszdyszy",
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^29.5.1",
		"jest": "^29.5.0",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.4"
	}
}